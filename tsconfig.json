{
  "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": ["ES2019"],
      "module": "commonjs",
      "target": "ES2019",
      "moduleResolution": "node",
      "noEmit": false,
      "strict": true,
      "skipLibCheck": true,
      "types": ["jest","node"],
      "noImplicitAny": false,
      "strictNullChecks": false,
      "outDir": "dest"
    },
    "include": ["index.js"],
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js",
      "src/__test__"
    ]
  }